  "bugs": {
    "url": "https://github.com/JoshSteward/restaurantLog/issues"
  },
  "homepage": "https://github.com/JoshSteward/restaurantLog#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "i": "^0.3.6",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "sequelize": "^6.6.2"
  }
}
